<div class="streamItem" data-tag="<%= tag %>">
    <div class="srcData">
        <p class="ip">
            <%= srcIP %>
        </p>
        <p class="port">
            <%= sourcePort %>
        </p>
    </div>
    <div class="separator">
        <span class="material-symbols-outlined">
            arrow_right_alt
        </span>
    </div>
    <div class="dstData">
        <p class="port">
            <%= destinationPort %>
        </p>
        <p class="ip">
            <%= dstIP %>
        </p>
    </div>
</div>